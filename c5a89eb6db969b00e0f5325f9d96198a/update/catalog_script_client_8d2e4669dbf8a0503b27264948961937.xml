        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var jsonData = g_form.getValue('fullfilment_policies');

    try {
        if (Array.isArray(JSON.parse(jsonData)))
            return true;
        else {
            g_form.addErrorMessage("Fullfilment policies are not a valid json");
            g_form.clearValue('fullfilment_policies');
            return false;
        }
    } catch (error) {
        g_form.addErrorMessage("Fullfilment policies are not valid json");
        g_form.clearValue('fullfilment_policies');
        return false;
    }
}]]></script>
